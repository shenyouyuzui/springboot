<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tkrs.mapper.CbondListMapper">
<!-- Result Map -->
	<resultMap id="baseResultMap" type="com.tkrs.domain.CbondListDomain">
		<result column="iterestfrequency" property="interestFrequency" />
		<result column="couponrate" property="couponRate" />
		<result column="rate_issueFormer" property="rateIssueFormer" />
		<result column="interest_type" property="interestType" />
		<result column="cn_name" property="cnName" />
		<result column="ipo_date" property="ipoDate" />
		<result column="rate_former" property="rateFormer" />
		<result column="paymenttype" property="paymentType" />
		<result column="bclc" property="bclc" />
		<result column="maturitydate" property="maturityDate" />
		<result column="par" property="par" />
		<result column="code" property="code" />
		<result column="sec_name" property="secName" />
		<result column="issueamount" property="issueAmount" />
		<result column="cn_bclc" property="cnBclc" />
	</resultMap>
	
	<select id="queryInfoByCode" resultMap="baseResultMap"  parameterType="java.lang.String">
		select iterestfrequency,couponrate,rate_issueformer,interesttype,cn_name,ipo_date,rate_former,
		paymenttype,bclc,maturitydate,par,code,sec_name,issueamount from c_bondlist_new where code=#{code} limit 0,1
	</select>
	
	
</mapper>